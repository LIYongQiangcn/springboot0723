<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.dao.ProductMapper">
    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultType="com.example.entity.Product">
        select * from product where id = id = #{id}
    </select>
    <update id="updateByPrimaryKey" parameterType="com.example.entity.Product">
        update product set num= #{num,jdbcType=INTEGER}
        where id = #{id,jdbcType=INTEGER}
    </update>

</mapper>